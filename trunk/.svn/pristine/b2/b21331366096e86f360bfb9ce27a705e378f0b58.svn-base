.AlignTopLeft { -fx-alignment: TOP-LEFT; }
.AlignTopCenter { -fx-alignment: TOP-CENTER; }
.AlignTopRight { -fx-alignment: TOP-RIGHT; }
.AlignCenterLeft { -fx-alignment: CENTER-LEFT; }
.AlignCenterCenter { -fx-alignment: CENTER-CENTER; }
.AlignCenterRight { -fx-alignment: CENTER-RIGHT; }
.AlignBottomLeft { -fx-alignment: BOTTOM-LEFT; }
.AlignBottomCenter { -fx-alignment: BOTTOM-CENTER; }
.AlignBottomRight { -fx-alignment: BOTTOM-RIGHT; }

.LoginTable {
   -fx-background-color: linear-gradient(#FAFAFA, #61a2b1);
   -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 );
   -fx-background-insets: 0, 10;
   -fx-background-radius: 10px, 0px;
}





     
     





