.LogoWaterMark { 
   -fx-background-image: '../img/logoEmpresaWM.png';
   -fx-background-size: 800, 600;
   -fx-background-repeat: no-repeat;
}

.BackgroundClass{
   -fx-background-color: #ffffff;
   -fx-background-image: '../img/body-bg.jpg';
   -fx-background-repeat: true;
}

.HeaderLabel { 
   -fx-font-family: VW Headline Black,Verdana,Arial,sans-serif; 
   -fx-font-size:18pt; 
   -fx-font-weight: bold; 
   -fx-text-fill:#004666; 
   -fx-text-align:center; 
}

.TitleLabel  { 
   -fx-font-family: VW Headline Black,Verdana,Arial,sans-serif; 
   -fx-font-size:15pt; 
   -fx-font-weight: bold; 
   -fx-text-fill:#004666; 
   -fx-text-align:center; 
}

.Title2Label { 
   -fx-font-family: VW Headline Black,Verdana,Arial,sans-serif; 
   -fx-font-size:12pt; 
   -fx-font-weight: bold; 
   -fx-text-fill:#004666; 
   -fx-text-align:center; 
}

.FooterLabel { 
   -fx-font-family: VW Headline Black,Verdana,Arial,sans-serif; 
   -fx-font-size:12pt; 
   -fx-font-weight: bold; 
   -fx-text-fill:#004666; 
   -fx-text-align:left; 
}

.FooterLabelSmall { 
   -fx-font-family: VW Headline Black,Verdana,Arial,sans-serif; 
   -fx-font-size:10pt; 
   -fx-font-weight: bold; 
   -fx-text-fill:#004666; 
   -fx-text-align:left;
}

.FieldLabel, .FieldLabelBig, .FieldLabelCenter, .FieldLabelBigCenter, .FieldLabelRight, .FieldLabelBigRight, .FieldLabelReq,
.FieldLabelSmall, .FieldLabelSmallLeft, .FieldLabelSmallCenter, .FieldLabelSmallRight,
.FieldLabelMedium, .FieldLabelMediumLeft, .FieldLabelMediumCenter, .FieldLabelMediumRight, .FieldLabelMediumReq { 
   -fx-font-weight: bold; 
   -fx-text-fill:#004666; 
}

.FieldLabelBig, .FieldLabelBigRight, .FieldLabelBigCenter { 
   -fx-font-size:12pt; 
}
.FieldLabelMedium, .FieldLabelMediumLeft, .FieldLabelMediumCenter, .FieldLabelMediumRight, .FieldLabelMediumReq { 
   -fx-font-size: 10pt; 
}
.FieldLabel, .FieldLabelLeft, .FieldLabelCenter, .FieldLabelRight, .FieldLabelReq { 
   -fx-font-size: 8pt; 
}
.FieldLabelSmall, .FieldLabelSmallCenter, .FieldLabelSmallRight { 
   -fx-font-size: 7pt; 
}

.FieldData, .FieldDataRight, .FieldDataCenter { 
   -fx-font-size: 8pt; 
   -fx-font-weight:normal; 
   -fx-fill-text:#004666; 
}
.FieldDataBig, .FieldDataBigRight, .FieldDataBigCenter { 
   -fx-font-size:10pt; 
   -fx-font-weight:normal; 
   -fx-fill-text:#004666;
   
}

.FieldLabelReq { 
   -fx-fill-text:#006384; 
   -fx-display: inline-block; 
   -fx-position: relative; 
   -fx-margin-left: .4em; 
   -fx-text-fill: red;
}
.FieldLabelReq:before { 
   -fx-content:"* "; 
   -fx-display: block; 
   -fx-position: absolute; 
   -fx-left: -.8em; 
   -fx-top: -.1em; 
}

.botones {
    -fx-border-radius: 2px;   
    -fx-font-size: 12px;
    -fx-text-fill: #ffffff;
    -fx-font-weight: bold;
    -fx-background-color: linear-gradient(#61a2b1, #2A5058);
    -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 );
    -fx-cursor:pointer; 
    -fx-background-position:3px 3px; 
    -fx-background-repeat:no-repeat; 
    -fx-display:inline-block; 
    -fx-padding:2px 2px 2px 21px; 
    -fx-margin:0px 5px 0px 5px;
    -fx-text-alignment:left;
}
.botones:hover {
   -fx-background-color: linear-gradient(#2A5058, #61a2b1);
}

