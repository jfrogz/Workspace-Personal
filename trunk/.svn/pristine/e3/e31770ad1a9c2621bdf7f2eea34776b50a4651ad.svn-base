<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="248.0" prefWidth="426.0"  xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox layoutX="175.0" layoutY="179.0" prefHeight="30.0" prefWidth="237.0" spacing="60">
         <children>
            <Button fx:id="btnGuardarPermiso" mnemonicParsing="false"   onAction="#guardarPermiso" styleClass="botones"/>
            <Button fx:id="btnCerrarPermiso" mnemonicParsing="false"    onAction="#cerrar" styleClass="botones"/>
         </children>
      </HBox>
      <GridPane layoutX="23.0" layoutY="46.0" prefHeight="114.0" prefWidth="380.0">
         <children>
            <Label fx:id="lbDesPermiso"  text="Descripción de la Función" GridPane.rowIndex="2" />
            <Label fx:id="lbEstatus"  text="Estatus:" GridPane.rowIndex="3" />
            <TextField fx:id="txtDesPermiso" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <CheckBox fx:id="chxEstatusPermiso" lineSpacing="7.0" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="3" selected="true"/>
            <Label fx:id="id" />
            <Label fx:id="idEtiqueta"/>
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="10.0" prefWidth="159.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="241.0" minWidth="10.0" prefWidth="241.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
   </children>
</AnchorPane>
