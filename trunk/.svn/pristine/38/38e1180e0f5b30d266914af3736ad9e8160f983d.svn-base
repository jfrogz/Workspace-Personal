       Status as activo
FROM FWK_CAT_CONTENT
WHERE FWK_CAT_CONTENT.Id_App = dbo.NUM_SISTEMA('SMS')
  AND FWK_CAT_CONTENT.Id_Admin = dbo.NUM_CATALOGO('SMS','PropiedadesEmpresa')
