<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <fx:include source="../configuration/HeaderTemplate.fxml" />
    </top>

    <bottom>
        <fx:include source="../configuration/FooterTemplate.fxml" />
    </bottom>
    
    <center>
        <BorderPane>
            <top>
                <GridPane layoutX="33.0" layoutY="44.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="524.0" minWidth="10.0" prefWidth="75.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="599.0" minWidth="10.0" prefWidth="162.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="462.0" minWidth="10.0" prefWidth="84.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="675.0" minWidth="10.0" prefWidth="205.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="517.0" minWidth="10.0" prefWidth="286.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="31.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="27.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="54.0" minHeight="10.0" prefHeight="18.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="41.0" minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <TextField fx:id="flClave" GridPane.columnIndex="1" />
                        <TextField fx:id="flEmpresa" prefHeight="22.0" prefWidth="507.0" GridPane.columnIndex="3" />
                        <Label fx:id="lblClave" GridPane.halignment="LEFT" GridPane.valignment="CENTER">
                            <padding>
                                <Insets right="10.0" />
                            </padding>
                        </Label>
                        <Label fx:id="lblEmpresa" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
                            <padding>
                                <Insets right="10.0" />
                            </padding>
                        </Label>
                        <Button fx:id="btnNuevo" layoutX="42.0" layoutY="100.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="71.0" styleClass="botones" GridPane.columnSpan="2" GridPane.rowIndex="2">
                            <GridPane.margin>
                                <Insets left="15.0" right="15.0" />
                            </GridPane.margin>
                        </Button>

                    </children>
                    <BorderPane.margin>
                        <Insets left="15.0" right="15.0" />
                    </BorderPane.margin>
                </GridPane>

            </top>
            <padding>
                <Insets bottom="10.0" top="10.0" />
            </padding>
            <center>
                <BorderPane fx:id="borderTabla" BorderPane.alignment="CENTER">
                    <center>
                        <TableView id="tablaDatos" fx:id="tablaDatos">
                            <columns>
                                <TableColumn fx:id="colId" minWidth="0.0" resizable="true" />                   
                                <TableColumn fx:id="colIdSAP" minWidth="0.0" resizable="true" />
                                <TableColumn fx:id="colClave" minWidth="0.0" resizable="true" />
                                <TableColumn fx:id="colNombreEmpresa" minWidth="0.0" resizable="true" />
                                <TableColumn fx:id="colRazonSocial" minWidth="0.0" resizable="true"  />
                                <TableColumn fx:id="colCliePro" minWidth="0.0" resizable="true" />
                                <TableColumn fx:id="colEstatus" minWidth="0.0" resizable="true" />
                                <TableColumn fx:id="colContingencia" minWidth="0.0" resizable="true" />
                                <TableColumn fx:id="colAcciones" minWidth="0.0" resizable="true"  />
                            </columns>                              
                        </TableView>               
                    </center>
                    <padding>
                      <Insets left="15.0" right="15.0" />
                    </padding>
               <bottom>
                  <GridPane fx:id="gridBotones" BorderPane.alignment="CENTER">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.halignment="RIGHT">
                           <children>
                              <ComboBox fx:id="comboPaginacion" nodeOrientation="LEFT_TO_RIGHT" />
                              <Label fx:id="lbPaginacion">
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                              </Label>
                           </children>
                        </HBox>
                     </children>
                  </GridPane>
               </bottom>  
                </BorderPane>
            </center>
        </BorderPane>
    </center>
</BorderPane>
